<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="projectNameSpace">
    <insert id="addproject" parameterType="map">
         INSERT INTO z_project(z_userid,z_name,z_time,z_passtime,z_docnum,address,type,p_url) VALUES (#{z_userid},#{z_name},#{z_time},#{z_passtime},#{z_docnum},#{address},#{type},#{url});
    </insert>
</mapper>