<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="projectNameSpace">
    <insert id="addproject" parameterType="map">
         INSERT INTO z_project(project_userid,project_name,project_time,project_passtime,project_docnum,project_address,project_street,project_type,project_url,project_context,project_state) VALUES (#{z_userid},#{z_name},#{z_time},#{z_passtime},#{z_docnum},#{local},#{add},#{type},#{url},#{filename},#{state});
    </insert>
    <select id="count" resultType="java.lang.Integer">
        SELECT COUNT(*) FROM z_project;
    </select>
    <select id="selectproject" parameterType="map" resultType="map">
        SELECT * FROM z_project limit #{pageNum},#{pageSize};
    </select>
</mapper>